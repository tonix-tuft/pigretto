{
  "env": {
    "module": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": "6.10"
            },
            "modules": false
          }
        ]
      ],
      "plugins": [
        "@babel/plugin-proposal-object-rest-spread",
        "@babel/plugin-proposal-class-properties"
      ]
    }
  },
  "sourceMaps": true
}
